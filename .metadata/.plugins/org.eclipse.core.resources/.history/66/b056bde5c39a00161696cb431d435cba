package mapa;

import juego.*;

public class Mapa {
	
	private Celda[][] M;
	
	
	
	public Mapa(){
		M = new Celda[20][20];
		
		for(int i=0; i<20; i++)
			for(int j=0; j<20; j++)
				M[i][j]=new Celda(i,j,this);
	}
	
	public void setObjeto(GameObject obj, int x, int y,int pos){
		M[x][y].setObjeto(obj, pos);
	}
	
	public GameObject getObjeto(int x, int y ,int pos){
		return M[x][y].getObjeto(pos);
	}
	
	

}
